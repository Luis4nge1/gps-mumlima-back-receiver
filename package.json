{
  "name": "gps-receiver-service",
  "version": "1.0.0",
  "description": "GPS Receiver Microservice for receiving and storing GPS coordinates in Redis with dual storage format.",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "example": "node examples/usage-example.js",
    "health": "curl -s http://localhost:3000/api/gps/health | jq",
    "stats": "curl -s http://localhost:3000/api/gps/stats | jq",
    "cleanup": "curl -X POST -s http://localhost:3000/api/gps/cleanup | jq"
  },
  "keywords": [
    "gps",
    "microservice",
    "redis",
    "bullmq",
    "express"
  ],
  "author": "luis4nge1",
  "license": "ISC",
  "dependencies": {
    "bullmq": "^5.56.4",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "prom-client": "^15.1.3",
    "rate-limit-redis": "^4.2.1",
    "redis": "^5.6.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
